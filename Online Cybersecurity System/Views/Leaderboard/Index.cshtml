@model IEnumerable<Score>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>Score Id</th>
        <th>Username</th>
        <th>Score </th>
        <th>Category</th>
        <th>Created Date</th>
        <th></th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.id</td>
            <td>@s.Employee.Username</td>
            <td>@s.Category.CategoryName</td>
            <td>@s.createDate </td>
        </tr>
    }
</table>